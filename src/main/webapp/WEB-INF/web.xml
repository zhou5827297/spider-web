<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         metadata-complete="true">

    <welcome-file-list>
        <welcome-file>spider_brower.html</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>spiderBrowerServlet</servlet-name>
        <servlet-class>com.spider.servlet.SpiderBrowerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>spiderBrowerServlet</servlet-name>
        <url-pattern>/spiderBrower</url-pattern>
    </servlet-mapping>

</web-app>